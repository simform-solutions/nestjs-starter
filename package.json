{
  "name": "nestjs-starter",
  "version": "0.0.1",
  "author": "hmake98",
  "license": "MIT",
  "main": "main.js",
  "scripts": {
    "prebuild": "rimraf build",
    "build": "nest build",
    "start": "node dist/main",
    "dev": "dotenv -e .env.local -- nest start --watch",
    "console:dev": "dotenv -e .env.development -- ts-node -r tsconfig-paths/register src/console.ts",
    "console:test": "dotenv -e .env.test -- ts-node -r tsconfig-paths/register src/console.ts",
    "console:prod": "ts-node -r tsconfig-paths/register src/console.ts",
    "lint": "eslint .",
    "lint:fix": "eslint --fix .",
    "test": "jest",
    "test:local": "dotenv -e .env.test -- jest",
    "test:local-e2e": "dotenv -e .env.test -- jest --config ./test/jest-e2e.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install",
    "pre-commit": "lint-staged"
  },
  "engines": {
    "node": "14.16.0"
  },
  "dependencies": {
    "@nestjs/bull": "^10.0.1",
    "@nestjs/common": "^10.2.4",
    "@nestjs/core": "^10.2.4",
    "@nestjs/jwt": "^10.1.1",
    "@nestjs/passport": "^10.0.1",
    "@nestjs/platform-express": "^10.2.4",
    "@nestjs/schedule": "^3.0.3",
    "@nestjs/swagger": "^7.1.10",
    "@nestjs/terminus": "^10.0.1",
    "@nestjs/throttler": "^5.0.0",
    "@nestjs/typeorm": "^10.0.0",
    "aws-sdk": "^2.1451.0",
    "bcrypt": "^5.1.1",
    "bull": "^4.11.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.1",
    "express-rate-limit": "^6.10.0",
    "firebase-admin": "^11.10.1",
    "helmet": "^7.0.0",
    "ioredis": "^5.3.2",
    "jest-junit": "^16.0.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "morgan": "^1.10.0",
    "nanoid": "^4.0.2",
    "nestjs-console": "^9.0.0",
    "nestjs-i18n": "^10.3.1",
    "nestjs-pino": "^3.4.0",
    "passport": "^0.6.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "pg": "^8.11.3",
    "pino-http": "^8.5.0",
    "pino-pretty": "^10.2.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^5.0.1",
    "rxjs": "^7.8.1",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "0.3.17",
    "typeorm-naming-strategies": "^4.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@nestjs/cli": "^10.1.17",
    "@nestjs/schematics": "^10.0.2",
    "@nestjs/testing": "^10.2.4",
    "@types/bcrypt": "^10.0.28-alpha",
    "@types/bull": "^3.15.9",
    "@types/copyfiles": "^2.4.1",
    "@types/eslint": "^8.44.2",
    "@types/eslint-plugin-prettier": "^3.1.0",
    "@types/i18n": "^0.13.6",
    "@types/ioredis": "^4.28.10",
    "@types/jest": "^29.5.4",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/lodash": "^4.14.198",
    "@types/morgan": "^1.9.5",
    "@types/node": "^20.5.9",
    "@types/passport-jwt": "^3.0.9",
    "@types/passport-local": "^1.0.35",
    "@types/pg": "^8.10.2",
    "@types/prettier": "^2.7.3",
    "@types/rimraf": "^3.0.2",
    "@types/supertest": "^2.0.12",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@typescript-eslint/parser": "^6.6.0",
    "copyfiles": "^2.4.1",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.3.0",
    "eslint": "^8.48.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-jest": "^27.2.3",
    "eslint-plugin-prettier": "^5.0.0",
    "husky": "^8.0.3",
    "jest": "^29.6.4",
    "lint-staged": "^14.0.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "ts-node": "10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "typescript": "5.2.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "./",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node",
    "reporters": [
      "default",
      [
        "jest-junit",
        {
          "outputDirectory": ".",
          "outputName": "junit.xml"
        }
      ]
    ]
  },
  "lint-staged": {
    "*.{js,ts}": "npm run lint:fix",
    "*.{ts,js,json,md}": "prettier --write"
  }
}
